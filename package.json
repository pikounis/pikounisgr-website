    "final-form": "^4.20.2",
    "firebase": "^8.6.5",
    "google-maps-react": "^2.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mui-rff": "^3.0.9",
    "notistack": "^1.0.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
