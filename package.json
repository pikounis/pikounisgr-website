  "description": "In the project directory, you can run:",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blue-Functor/react-template.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Blue-Functor/react-template/issues"
  },
  "homepage": "https://github.com/Blue-Functor/react-template#readme",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js --open --host 0.0.0.0",
    "build": "webpack --config webpack/webpack.prod.js",
    "analyze": "webpack --env analyze=true --config webpack/webpack.prod.js",
    "test": "jest --watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-flow"
    ],
    "plugins": [
      [
        "transform-imports",
        {
          "lodash": {
            "transform": "lodash/${member}",
            "preventFullImport": true
          },
          "@material-ui/core": {
            "transform": "@material-ui/core/esm/${member}",
            "preventFullImport": true
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^2.10.11",
    "@material-ui/core": "^4.12.3",
